{
  "name": "@jogi47/pm-cli-plugin-asana",
  "version": "0.2.0",
  "description": "Asana plugin for pm-cli",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "oclif": {
    "commands": {
      "strategy": "pattern",
      "target": "./dist/commands"
    }
  },
  "scripts": {
    "build": "tsc -b",
    "dev": "tsc -b --watch",
    "clean": "rm -rf dist",
    "lint": "eslint src"
  },
  "dependencies": {
    "@jogi47/pm-cli-core": "workspace:*",
    "asana": "^3.0.0"
  },
  "peerDependencies": {
    "@oclif/core": "^4.0.0"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "@types/node": "^20.0.0"
  }
}
